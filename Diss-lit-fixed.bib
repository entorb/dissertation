doi = {10.1002/pip.2226},
file = {:home/torben/Documents/Mendeley Desktop/Anctil et al.-2012-Cumulative energy demand for small molecule and polymer photovoltaics.pdf:pdf},
issn = {10627995},
journal = {Progress in Photovoltaics: Research and Applications},
keywords = {PV general,embodied energy,life cycle assessment,organic photovoltaics,semiconductor},
mendeley-tags = {PV general},
