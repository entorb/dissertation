@incollection{RiedeLuessemLeo2011,
abstract = {Organic semiconductors are a novel materials class which has recently been investigated in detail due to the very promising applications possibilities. In contrast to the classical inorganic semiconductors, which are usually used in single-crystalline forms, organic semiconductors consisting of either oligomer or polymer chain molecules are typically deposited as thin films on large areas leading to amorphous or polycrystalline structures. Since flexible and transparent substrates can be used, organic electronics open many new application and integration perspectives. With contributions from chemistry, physics, and engineering, organic semiconductors are an archetype for an interdisciplinary field. Organic semiconductors are characterized by the comparatively weak van der Waals bonding, leading to soft materials and narrow energy bands. In single crystals, band transport has been observed. In thin films, however, disorder plays an important role and hopping transport with mobilities well below 1 cm2 V−1 s−1 is typically observed. The optical properties are characterized by high absorption and emission rates, induced by the highly polarizable $\pi$-electronic systems and the large electron-hole correlation leading to exciton binding energies of about 0.5 eV. In this chapter, we address the basic properties of organic semiconductors, in particular their transport and optical properties. Then, we discuss device applications, with the goal to work out the basic principles of device operation and describe the current state of the art.},
author = {Riede, Moritz and L{\"{u}}ssem, Bj{\"{o}}rn and Leo, Karl},
booktitle = {Comprehensive Semiconductor Science and Technology},
chapter = {4.13},
doi = {10.1016/B978-0-44-453153-7.00123-1},
editor = {Bhattacharya, Pallab and Fornari, Roberto and Kamimura, Hiroshi},
isbn = {978-0-444-53153-7},
pages = {448--507},
publisher = {Elsevier Science},
title = {Organic Semiconductors},
url = {http://dx.doi.org/10.1016/B978-0-44-453153-7.00123-1},
year = {2011}
}
