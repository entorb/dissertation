journal = {Solar Energy Materials and Solar Cells},
keywords = {Fullerene C60,Organic photovoltaics,Solar cells,Transparent electrode},
pages = {165--170},
title = {Highly doped fullerene {C}$_{60}$ thin films as transparent stand alone top electrode for organic solar cells},
url = {http://dx.doi.org/10.1016/j.solmat.2013.08.017},
volume = {118},
year = {2013}
}
