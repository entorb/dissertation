journal = {Physical Review B},
month = sep,
number = {6},
pages = {3105--3113},
title = {Hopping transport in prototypical organic glasses},
url = {http://dx.doi.org/10.1103/PhysRevB.26.3105},
volume = {26},
year = {1982}
}
@article{Matis2010,
